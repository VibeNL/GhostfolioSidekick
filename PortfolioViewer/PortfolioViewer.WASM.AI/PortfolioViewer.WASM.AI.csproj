		<PackageReference Include="Microsoft.SemanticKernel" Version="1.67.1" />
		<PackageReference Include="Microsoft.SemanticKernel.Agents.Abstractions" Version="1.67.1" />
		<PackageReference Include="Microsoft.SemanticKernel.Agents.Core" Version="1.67.1" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\..\AI\AI.Agents\AI.Agents.csproj" />
	  <ProjectReference Include="..\..\AI\AI.Common\AI.Common.csproj" />
	  <ProjectReference Include="..\..\Database\Database.csproj" />
	  <ProjectReference Include="..\..\ExternalDataProvider\ExternalDataProvider.csproj" />
	</ItemGroup>
	
</Project>
